<?xml version="1.0" encoding="utf-8"?>
<extensionset
    name="Раздел Maxposter"
    description="Joomla! extensions for Maxposter.ru">

    <!-- Набор клиентских библиотек -->
    <extension name="Раздел MaxPoster: клиентские библиотеки"
        element="lib_maxposter"
        type="library"
        version="1.0.2"
        detailsurl="http://maxposter-ru.github.com/J-extensions/library.xml"
    />
    <!-- Список авто -->
    <extension name="Раздел MaxPoster"
        element="com_maxposter"
        type="component"
        version="17.0.3.2"
        detailsurl="http://maxposter-ru.github.com/J-extensions/component.xml"
    />

    <!-- Список марок и моделей -->
    <extension name="Раздел MaxPoster: марки-модели"
        element="mod_maxposter_manufacturers"
        type="module"
        version="17.0.1"
        detailsurl="http://maxposter-ru.github.com/J-extensions/module_manufacturers.xml"
    />
    <!-- Форма поиска -->
    <extension name="Раздел MaxPoster: форма поиска"
        element="mod_maxposter_search"
        type="module"
        version="17.0.1"
        detailsurl="http://maxposter-ru.github.com/J-extensions/module_search.xml"
    />

</extensionset>
